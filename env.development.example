###### Basic app settings
NODE_ENV=development
LISTEN_HOST=localhost
LISTEN_PORT=3000

###### Timezone
# Force application timezone (affects pretty logs)
TZ=UTC

###### API prefix
API_BASE_PATH=api
API_VERSION=v1

###### HTTP client settings
# Timeout for outbound provider requests in SECONDS
REQUEST_TIMEOUT_SEC=60

###### LLM defaults
# Default max_tokens when not provided in request
LLM_MAX_TOKENS_DEFAULT=1024

###### Logging
# Development uses more verbose logging
LOG_LEVEL=debug
